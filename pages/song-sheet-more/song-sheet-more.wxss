/* pages/song-sheet-more/song-sheet-more.wxss */
.sheet-item{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  margin: 30rpx 10rpx 0 10rpx;
}
.song-sheet-item{
  width: 220rpx;
  height: 220rpx;
  padding-bottom: 120rpx;
  position: relative;
}
.song-sheet-item image{
  width: 100%;
  height: 100%;
  border-radius: 30rpx;
}
.name{
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 120rpx;
  font-size: 26rpx;
  padding: 0 10rpx;
  /* overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; */
}