.tabbar_box{
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 110rpx;
    box-shadow: 0 0 5px #32cd32;
}
.tabbar_box.iphoneX-height{
    padding-bottom: 66rpx;
}
.middle-wrapper{
  position: absolute;
  right: 310rpx;
  bottom: 0;
  background-color: #fff;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border-top: 2rpx solid #f2f2f3;
}
.middle-wrapper.iphoneX-height{
  bottom: 66rpx;
}
.tabbar_nav{
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 24rpx;
    height: 100%;
    position: relative;
}
.tabbar_icon{
    width: 60rpx;
    height: 60rpx;
}
.special-wrapper{
  position: absolute;
  left: 50%;
  margin-left: -60rpx;
  top: -16rpx;
  /* width: 112rpx;
  height: 112rpx; */
  border-radius: 50%;
  background-color: #fff;
  text-align: center;
  box-sizing: border-box;
  /* padding: 2rpx; */
  box-shadow: 0 -2px 1px rgb(50, 205, 50, .4);
}
.special-wrapper .tabbar_icon{
    width: 120rpx;
    height: 120rpx;
}
.special-text-wrapper{
  width: 56rpx;
  height: 56rpx;
}